{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "amd": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "no-const-assign": 2,
    "camelcase": 0,
    "comma-dangle": [2, "never"],
    "comma-spacing": 0,
    "comma-style": [2, "last"],
    "complexity": [0, 11],
    "default-case": 2,
    "no-duplicate-case": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "valid-typeof": 2,
    "semi": [2, "always"],
    "strict": 2,
    "use-isnan": 2,
    "curly": [2, "all"],
    "eqeqeq": 0,
    "init-declarations": 0,
    "key-spacing": [0, { "beforeColon": false, "afterColon": true }],
    "max-params": [0, 4],
    "newline-after-var": 0,
    "quotes": [ 1, "single" ],
    "no-undef": 1,
    "global-strict": 0,
    "no-extra-semi": 1,
    "no-underscore-dangle": 0,
    "no-console": 0,
    "no-extra-parens": 0,
    "no-empty": 1,
    "no-unused-vars": 1,
    "no-trailing-spaces": [1, { "skipBlankLines": true }],
    "no-unreachable": 1,
    "no-alert": 0,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "no-lone-blocks": 0,
    "no-labels": 2
  }
}
